{
  "name": "Time aware Tab Manager",
  "version": "1.0",
  "manifest_version": 3,
  "permissions": [
    "alarms",
    "notifications",
    "storage",
    "tabs",
    "tabGroups",
    "idle"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_title": "Time aware Tab Manager",
    "default_popup": "popup.html"
  },
  "icons": {
    "16": "icons/16.png",
    "32": "icons/32.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  }
}